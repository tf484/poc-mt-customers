{
  "name": "poc-mt-customers",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^8",
    "@cap-js/hana": "^1",
    "@sap/cds-mtxs": "^2",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.6",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8",
    "@sap/ux-specification": "^1.120.17",
    "rimraf": "^5.0.5",
    "npm-run-all": "^4"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-pocmtcustomers": "cds watch --open pocmtcustomers/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy poc-mt-customers --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "build-deploy": "run-s build deploy",
    "login-poc": "../../scripts/cf-aic-poc-login.sh",
    "build-deploy-poc": "run-s login-poc build-deploy",
    "login-dev": "../../scripts/cf-aic-dev-login.sh",
    "build-deploy-dev": "run-s login-dev build-deploy",
    "login-test": "../../scripts/cf-aic-test-login.sh",
    "build-deploy-test": "run-s login-test build-deploy"
  },
  "cds": {
    "requires": {
      "[production]": {
        "db": "hana",
        "auth": "xsuaa",
        "multitenancy": true,
        "extensibility": false,
        "toggles": false
      }
    }
  },
  "sapux": [
    "app/pocmtcustomers"
  ]
}
